config_version=5

[application]

config/name="Incandescent"
config/description="Taking inspiration from Dani's Fair 'n Square and Askiisoft's Katana:ZERO, Incandescent is a 2d side-view focused on frame-perfect inputs in which you try to survive the bad guys and take back your cat."
run/main_scene="res://scenes/Main.tscn"
config/features=PackedStringArray("4.0", "C#", "Forward Plus")
run/max_fps=240
run/frame_delay_msec=1

[autoload]

CoroutineManager="*res://scenes/managers/CoroutineManager.tscn"
GameManager="*res://scenes/managers/GameManager.tscn"
LevelManager="*res://scenes/managers/LevelManager.tscn"

[display]

window/size/viewport_width=320
window/size/viewport_height=180
window/size/window_width_override=1280
window/size/window_height_override=720
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=0
window/stretch/mode="canvas_items"

[dotnet]

project/assembly_name="Incandescent"

